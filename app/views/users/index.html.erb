<%= render 'books/info', user: @user, book: @book %>
<h2>Users</h2>
<table>
  <tr>
    <th>Image</th>
    <th>Name</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show",user_path(user) %></td>
    </tr>
  <% end %>
</table>